{
  "name": "@artifact/fly-agent",
  "exports": "./main.ts",
  "tasks": {
    "watch": "deno test -A --trace-leaks --watch",
    "test": "deno test -A",
    "lint": "deno lint --fix",
    "fmt": "deno fmt",
    "ok": "deno check && deno task test && deno fmt --check && deno lint",
    "web": "bash test.sh",
    "dev": "deno run -A --watch main.ts"
  }
}

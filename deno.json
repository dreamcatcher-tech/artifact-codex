{
  "workspace": [
    "mcp-agents",
    "mcp-computers",
    "mcp-faces",
    "mcp-interactions",
    "face-cmd",
    "face-codex",
    "face-inspector",
    "face-test",
    "web-server",
    "shared"
  ],
  "tasks": {
    "ok": "deno task --filter='@artifact/' ok"
  },
  "imports": {
    "debug": "npm:debug@^4.4.3"

  "fmt": {
    "semiColons": false,
    "singleQuote": true,
    "lineWidth": 80,
    "exclude": [".refs/"]
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "exclude": [".refs/"],
  "nodeModulesDir": "auto"
}

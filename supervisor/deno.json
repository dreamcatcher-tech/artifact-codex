{
  "name": "@artifact/supervisor",
  "version": "0.0.1",
  "exports": {
    ".": "./mod.ts",
    "./fixture": "./fixture.ts"
  },
  "tasks": {
    "dev": "DEBUG='@artifact/*' deno run -A --watch dev.ts",
    "mcp": "DEBUG='@artifact/*' deno run -A mcp.ts",
    "test": "deno test -A",
    "watch": "deno test -A --watch --trace-leaks",
    "lint": "deno lint --fix",
    "fmt": "deno fmt",
    "ok": "deno check && deno task test && deno fmt --check && deno lint"
  }
}

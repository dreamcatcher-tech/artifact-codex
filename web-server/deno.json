  },
  "imports": {
    "@hono/mcp": "jsr:@dreamcatcher/artifact-mcp@^0.1.3",
    "@hono/hono": "jsr:@hono/hono@^4.9.6",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.17.5",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/path": "jsr:@std/path@^1.1.2",
    "serialize-error": "npm:serialize-error@^12.0.0",
    "ws": "npm:ws@^8.18.3",
    "zod": "npm:zod@^3.25.76"
  }
}
